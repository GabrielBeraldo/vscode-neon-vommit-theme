			"scope": "keyword.operator.new",
			"settings": {
				"foreground": "#FF00AA"
			}
		},
		{
			"name": "Storage",
			"scope": "storage, storage.modifier, storage.type.function",
			"settings": {
				"foreground": "#FF00AA"
			}
