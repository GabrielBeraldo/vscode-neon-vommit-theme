{
  "name": "Neon Vommit",
	"type": "dark",
	"colors": {
		"editor.background": "#222222",
		"editor.foreground": "#f0f0f0",
		"editor.selectionBackground": "#646464",
		"editor.lineHighlightBackground": "#3c3c3c80",
		"editorCursor.foreground": "#f0f0f0",
		"editorWhitespace.foreground": "#4a4a4a",
		"editorRuler.foreground": "#3a3a3a",
		"terminal.foreground": "#33cc33",
		"terminalCursor.foreground": "#4455bb",
		"terminal.ansiBlack": "#102210"
	},
	"tokenColors": [
		{
			"settings": {
				"foreground": "#f0f0f0ff",
				"background": "#222222"
			}
		},
		{
			"name": "Text",
			"scope": "text.xml",
			"settings": {
				"foreground": "#f0f0f0"
			}
		},
		{
			"name": "Comment",
			"scope": "comment",
			"settings": {
				"foreground": "#BBAA99"
			}
		},
		{
			"name": "String",
			"scope": "string, string.value, string.quoted.single.scss, string.quoted.double.json, string.quoted.double.html, string.quoted.double.php,\r\n          meta.return-type, meta.type.cast.expr, meta.type.new.expr, meta.preprocessor.string, support.constant.math, support.constant.dom,\r\n          support.constant.json, markup.inline.raw, invalid.illegal.bad-ampersand.html, constant.character.escape.scss",
			"settings": {
				"foreground": "#CBC3FF"
			}
		},
		{
			"name": "REGEXP",
			"scope": "constant.regexp, string.regexp, string.regexp.double-quoted.php",
			"settings": {
				"foreground": "#F0F0F0"
			}
		},
		{
			"name": "Number",
			"scope": "constant.numeric, meta.preprocessor.numeric, keyword.other.unit, constant.sha.git-rebase",
			"settings": {
				"foreground": "#33CC99"
			}
		},
		{
			"name": "Built-in constant",
			"scope": "constant.language",
			"settings": {
				"foreground": "#4499FF"
			}
		},
		{
			"name": "User-defined constant",
			"scope": "constant.character, constant.other",
			"settings": {
				"foreground": "#4499FF"
			}
    },
    {
      "name": "JSDOC Types",
      "scope": "entity.name.type.instance.jsdoc",
      "settings":{
        "foreground": "#00F9AC"
      }
    },
    {
      "name": "JSDOC tags",
      "scope": "storage.type.class.jsdoc",
      "settings":{
        "foreground": "#A467CC"
      }
    },
		{
			"name": "Variable",
			"scope": "\r\n          variable, variable.css, variable.scss, variable.other.less, variable.language,\r\n          support.other.variable, entity.name.function, storage.type.cs, storage.type.generic.cs, storage.type.modifier.cs,\r\n          storage.type.variable.cs, storage.type.annotation.java, storage.type.generic.java, storage.type.java, storage.type.object.array.java,\r\n          storage.type.primitive.array.java, storage.type.primitive.java, storage.type.token.java, storage.type.groovy, storage.type.annotation.groovy,\r\n          storage.type.parameters.groovy, storage.type.generic.groovy, storage.type.object.array.groovy, storage.type.primitive.array.groovy,\r\n          storage.type.primitive.groovy, variable.other.object.js\r\n        ",
			"settings": {
				"foreground": "#66D9EF"
			}
		},
		{
			"name": "Variable Declaration",
			"scope": "\r\n          entity.name, support.type.property-name, support.type.vendored.property-name, meta.definition.variable.name, meta.object-literal.key,\r\n        variable.other.readwrite.js",
			"settings": {
				"foreground": "#66D9EF"
			}
    },
    {
      "name": "JS Object Property",
      "scope": "variable.other.object.property.js, variable.other.property.js",
      "settings":{
        "foreground": "#BB77FF"
      }
    },
		{
			"name": "Keyword",
			"scope": "keyword, keyword.control, keyword.other",
			"settings": {
				"foreground": "#FF00AA"
			}
    },
    {
      "name": "JS Operator Keywords",
      "scope": "keyword.operator.logical.js, keyword.operator.relational.js, keyword.operator.comparison.js, keyword.operator.expression",
      "settings":{
        "foreground": "#FFFF00"
      }
    },
		{
